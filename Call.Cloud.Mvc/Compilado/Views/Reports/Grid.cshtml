

@model Call.Cloud.Mvc.Models.ReportsVM.ListarReports
@{
    ViewBag.Title = "Grid";
    Layout = "";
}
<div class="page animsition" style="animation-duration: 800ms; opacity: 1;">
    <div class="page-header">
        <strong>Reporte de empresa</strong>

        <div class="page-header-actions">

        </div>

    </div>
    <div class="panel-body">
        <div id="divGridPagina" class="table-responsive">
            @Html.Hidden("items_count", Model.Elementos.Count())
            <table class="table table-striped table-condensed text-center">
                <tr>
                    <th class="text-center col-sm-3">
                        <a class="punteroMouse">Name</a>
                    </th>
                    <th class="text-center col-sm-3">
                        <a class="punteroMouse">Year</a>
                    </th>
                    <th class="text-center col-sm-2">
                        <a class="punteroMouse">Quantity</a>
                    </th>


                </tr>

                @for (var i = 0; i < Model.Elementos.Count(); i++)
                {
                    var item = Model.Elementos.ToList()[i];
                    <tr>
                        <td class="text-center col-sm-1">@item.name_enterprise</td>
                        <td class="text-center col-sm-2">@item.año</td>
                        <td class="text-center col-sm-1">@item.cantidad</td>
                        <td class="text-right col-sm-1"></td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>
<script src="~/Scripts/usados/Report_Mostarjs.js"></script>
<script>
    (function () {
        iniciar();
    })();

</script>





