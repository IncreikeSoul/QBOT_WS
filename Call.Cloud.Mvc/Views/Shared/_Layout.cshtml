        //var GetStudentsURL = '<%: Url.Action("ListarAlertaJson", "JsonOperations") %>';
        $(document).ready(function () {
            try {
                //$.getJSON("/Login_user/ListarAlertaJson", null, function (data) {
                $.getJSON("http://localhost/ListenToMe/Login_user/ListarAlertaJson", null, function (data) {
                    try {
                        var dtPersons = eval(data);
                        displayJSONData(dtPersons);
                    }
                    catch (ex1) {
                        alert("ex1");
                    }
                });
            }
            catch (ex) {
                //alert(ex);
            }
            /******* Notificaciones ********/

            try {
                //$.getJSON("/Login_user/ListarNotificacionJson", null, function (data) {
                $.getJSON("http://localhost/ListenToMe/Login_user/ListarNotificacionJson", null, function (data) {
                try {

                        var dtNotificaciones = eval(data);
                        displayJSONDataNotificaciones(dtNotificaciones);
                    }
