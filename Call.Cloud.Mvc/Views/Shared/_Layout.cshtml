                        //alert(ex1);
                    }
                });
            }
            catch (ex) {
                //alert(ex);
            }

            });
        function displayJSONData(dtPersons) {
            var strColumnData1 = "";

            for (i = 0; i < dtPersons.length; i++)
            {
                var divRow = document.getElementById("divRow_" + i);
                if (i < dtPersons.length) {
                    strColumnData1 = dtPersons[i].NombreAlerta;
                    var user = '<a class="list-group-item" href="javascript:void(0)" role="menuitem"><div class="media"><div class="media-left padding-right-10"><i class="icon md-receipt bg-red-600 white icon-circle" aria-hidden="true"></i></div><div class="media-body"><h6 class="media-heading">' + dtPersons[i].NombreAlerta + '</h6><time class="media-meta" datetime="2015-06-12T20:50:48+08:00">' + dtPersons[i].tiempo + ' Hrs</time></div></div></a>';
                    $('#Divalerta table').append(user);
                }
            }
            var NumAlerta = '<span class="badge badge-danger up">' + dtPersons.length + '</span>';
            $('#DivNumalerta').append(NumAlerta);
        }
        function displayJSONDataNotificaciones(dtNotificaciones) {
            var strColumnData1 = "";

            for (i = 0; i < dtNotificaciones.length; i++) {

                if (i < dtNotificaciones.length) {
                    strColumnData1 = dtNotificaciones[i].NombreAlerta;
                    var user = '<a class="list-group-item" href="javascript:void(0)" role="menuitem"><div class="media"><div class="media-left padding-right-10"><span class="avatar avatar-sm avatar-online"><i></i></span></div><div class="media-body"><div class="media-meta"><time datetime="2015-06-17T20:22:05+08:00">' + dtNotificaciones[i].tiempo + ' Hrs</time></div><div class="media-detail">' + dtNotificaciones[i].NombreAlerta + '</div></div></div></a>';
                    $('#DivNotificaciones table').append(user);
                }
            }

            var NumNotificaciones = '<span class="badge badge-info up">' + dtNotificaciones.length + '</span>';
            $('#DivNumNotificaciones').append(NumNotificaciones);
        }
    </script>

