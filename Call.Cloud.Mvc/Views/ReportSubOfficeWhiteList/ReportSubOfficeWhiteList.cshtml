    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
        google.load("visualization", "1", { packages: ['corechart'] });
        function grafica01() {
            var parametros = {
                parm_Pk_office: $("#Filtro_parm_Pk_office").val(),
            }
            $(function () {
                $.ajax({
                    type: 'POST',
                    dataType: 'json',
                    url: '@Url.Action("chart_whiteSubOffice_Yaer")',
                    data: { filtro: parametros },
                    success: function (data) {
                        var data1 = new google.visualization.DataTable();
                        data1.addColumn('string', 'mes');
                        data1.addColumn('number', 'Porcentajes de palabras bien dichas');
                        data1.addColumn('number', 'Porcentaje de palabras mal dichas');
                        for (var i = 0; i < data.length; i++) {
                            data1.addRow([data[i].name_subOffice, data[i].portCumpli, data[i].PortnoCumpl]);;
                        }

                        console.log(data);
                        var options = {
                            title: "Reporte de año por Sub oficina",
                            legend: { position: 'top', maxLines: 15 },
                            fontsize: "100px;",
                            chartArea: { width: '90%', },
                            backgroundColor: 'transparent',
                            curveType: "function",
                            tooltip: { trigger: 'selection' },
                            isStacked: true,
                            vAxis: { format: '#\'%\'' },
                            annotations: {
                                boxStyle: {
                                    stroke: '#888',
                                    strokeWidth: 1,
                                    rx: 10,
                                    ry: 10,
                                    gradient: {
                                        color1: '#fbf6a7',
                                        color2: '#33b679',
                                        x1: '0%', y1: '0%',
                                        x2: '100%', y2: '100%',
                                        useObjectBoundingBoxUnits: true
                                    }
                                }
                            },
                        }
                        var view = new google.visualization.DataView(data1);
                        view.setColumns([0, 1, {
                            calc: "stringify",
                            sourceColumn: 1,
                            type: "string",
                            role: "annotation"

                        }, 2,
                                        {
                                            calc: "stringify",
                                            sourceColumn: 2,
                                            type: "string",
                                            role: "annotation"
                                        }
                        ]);

                        $("#div_chart").css("height", "400");
                        var chart = new google.visualization.ColumnChart(document.getElementById("div_chart"));
                        chart.setAction({
                            id: 'Business',
                            text: 'Business',
                            action: function () {
                                data1.setCell(chart.getSelection()[0].row, 1,
                                data1.getValue(chart.getSelection()[0].row, 1));
                                var name = data1.getValue(chart.getSelection()[0].row, 0);
                                for (var i = 0; i < data.length; i++) {
                                    var array1 = new Array();
                                    if (data[i].name_subOffice == name) {
                                        var array = new Array();
                                        array1 = [data[i].pk_subOffice];
                                        data[i].pk_subOffice = array1[i];
                                        array = [data[i].año];
                                        data[i] = array[i];
                                        var añoo = array;
                                        var pk_subOffice = array1;
                                        
                                    }
                                }
                                var pkSubOffice = pk_subOffice;
                                var Year = añoo;
                                chart.draw(view, options);
                                if (pk_subOffice == pkSubOffice && añoo == Year) {
                                    if (document.getElementById("chart01")) {
                                        document.getElementById("chart01").style.display = 'none';
                                        document.getElementById("chart02").style.display = 'none';
                                        document.getElementById("chart03").style.display = 'none';
                                        grafica01_Business_year(pkSubOffice, añoo);
                                    }
                                }

                            }
                        });
                        chart.setAction({
                            id: 'Mes',
                            text: 'Month',
                            action: function () {
                                data1.setCell(chart.getSelection()[0].row, 1,
                                data1.getValue(chart.getSelection()[0].row, 1));
                                var name = data1.getValue(chart.getSelection()[0].row, 0);
                                for (var i = 0; i < data.length; i++) {
                                    if (data[i].name_subOffice == name) {
                                        var array = new Array();
                                        var array1 = new Array();
                                        array1 = [data[i].pk_subOffice];
                                        array = [data[i].año];
                                        data[i].pk_subOffice = array1[i];
                                        data[i] = array[i];
                                        var añoo = array;
                                        var pksuboffice = array1;

                                    }
                                }
                                var pk_suboffice = pksuboffice;
                                var year = añoo;
                                chart.draw(view, options);
                                if (pksuboffice == pk_suboffice && year == añoo) {
                                    if (document.getElementById("chart01")) {
                                        document.getElementById("chart01").style.display = 'none';
                                        grafica02(pk_suboffice, añoo);
                                    }
                                }
                            }
                        })

                        chart.draw(view, options);
                    },
                    error: function () {
                        console.log(data);
                    }
                })
            });
        }
    </script>




